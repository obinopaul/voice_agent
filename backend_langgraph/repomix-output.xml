        self.api_key = api_key
        self.base_url = "https://api-football-v1.p.rapidapi.com/v3"

    def get_league_id(self, league_name: str) -> Dict[str, Any]:
        headers = {
            "x-rapidapi-host": "api-football-v1.p.rapidapi.com",  # RapidAPI host
